# See `info info infokey` for more vi-keys

j		next-line
k		prev-line
l		forward-char
h		backward-char
^e		down-line
^y		up-line
\kD		scroll-forward-page-only # PgDown
\kU		scroll-backward-page-only # PgUp
g		beginning-of-node
G		end-of-node
\kh		beginning-of-node # Home
\ke		end-of-node # End
-		history-node
n		next-node
p		prev-node
t		top-node
d		dir-node
\mu		clear-search

^o              history-node

:q              quit
ZZ              quit


#var
highlight-searches=On
link-style=yellow
active-link-style=yellow,bold
match-style=underline,bold,cyan
